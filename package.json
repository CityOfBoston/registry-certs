{
  "name": "registry-certs",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfBoston/registry-certs.git"
  },
  "license": "CC0-1.0",
  "engines": {
    "node": "8.1.4",
    "npm": "^5.0.0"
  },
  "scripts": {
    "dev": "babel-watch -D -w server server",
    "test": "jest --forceExit",
    "test-ci": "jest --coverage --runInBand --forceExit",
    "report-coverage": "codecov",
    "build": "gulp build",
    "heroku-postbuild": "gulp build",
    "bundle-deploy": "./scripts/bundle-deploy.sh",
    "report-deploy": "./scripts/report-deploy.sh",
    "start": "node build/server",
    "storybook": "start-storybook -p 9001 -s static -c storybook",
    "snapshot": "build-storybook -s static -c storybook && percy-storybook --widths=320,840,1280",
    "lint": "eslint --fix .",
    "clear-babel-cache": "rm -rf node_modules/.cache/babel-loader",
    "generate-fixtures": "babel-node ./scripts/generate-fixtures",
    "precommit": "lint-staged",
    "prepush": "flow --quiet && jest --silent --no-cache --noStackTrace --lastCommit"
  },
  "lint-staged": {
    "*.js": "eslint --quiet"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/\\.next/",
      "<rootDir>/coverage/"
    ],
    "collectCoverageFrom": [
      "{client,lib,pages,server}/**/*\\.js",
      "!**/*.stories.js",
      "!lib/test/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "json"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "boom": "^5.1.0",
    "clear-require": "^2.0.0",
    "compression": "^1.6.2",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "fs-extra": "^3.0.1",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "graphql": "^0.10.1",
    "graphql-server-hapi": "^1.0.0",
    "graphql-tools": "^1.0.0",
    "hapi": "^16.1.1",
    "http-status": "^1.0.1",
    "https-proxy-agent": "^2.0.0",
    "inert": "^4.2.0",
    "isomorphic-fetch": "^2.2.1",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "mobx-utils": "^2.0.1",
    "mssql": "^4.0.4",
    "next": "^3.0.1-beta.2",
    "node-cleanup": "^2.1.2",
    "nprogress": "^0.2.0",
    "opbeat": "^4.14.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-proxy": "https://github.com/finneganh/react-proxy/releases/download/fix-get-calling/react-proxy-3.0.0-alpha.1.tgz",
    "url-search-params": "^0.9.0"
  },
  "devDependencies": {
    "@percy-io/react-percy-storybook": "^1.0.0",
    "@storybook/addon-storyshots": "^3.1.6",
    "@storybook/react": "^3.1.7",
    "apollo-codegen": "^0.15.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-watch": "^2.0.6",
    "cheerio": "^0.22.0",
    "codecov": "^2.2.0",
    "del": "^3.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "fetch-mock": "^5.10.0",
    "flow-bin": "^0.50.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.3.0",
    "gulp-ignore": "^2.0.2",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.8",
    "husky": "^0.14.1",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.0.0",
    "prettier": "^1.4.4",
    "prop-types": "^15.5.10",
    "react-test-renderer": "^15.5.4",
    "through2": "^2.0.3",
    "webpack-bundle-analyzer": "^2.8.1"
  }
}
